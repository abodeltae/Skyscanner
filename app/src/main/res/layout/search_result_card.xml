<?xml version="1.0" encoding="utf-8"?>
<merge
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools"
        tools:parentTag="androidx.cardview.widget.CardView">
    <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingBottom="8dp"
            android:paddingTop="8dp">
        <ImageView
                android:layout_width="64dp"
                android:layout_height="64dp"
                tools:srcCompat="@tools:sample/avatars"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                android:id="@+id/imageViewOutBoundCarrier"
                android:layout_marginStart="8dp"
        />
        <TextView
                tools:text="TextView"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:id="@+id/textViewOutBoundTime"
                app:layout_constraintEnd_toStartOf="@+id/textViewOutboundType"
                app:layout_constraintStart_toEndOf="@+id/imageViewOutBoundCarrier"
                app:layout_constraintTop_toTopOf="parent"
                android:textColor="@android:color/black"
                android:layout_margin="8dp"/>
        <TextView
                tools:text="TextView"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toBottomOf="@id/textViewOutBoundTime"
                app:layout_constraintStart_toStartOf="@id/textViewOutBoundTime"
                android:id="@+id/textViewOutboundPlacesAndCarrier"
                app:layout_constraintEnd_toStartOf="@+id/textViewOutboundDuration" android:layout_marginTop="8dp"/>

        <TextView
                tools:text="TextView"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"

                android:textColor="@android:color/black"
                android:id="@+id/textViewOutboundType"
                android:layout_margin="8dp"/>
        <TextView
                tools:text="TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toBottomOf="@id/textViewOutboundType"
                app:layout_constraintEnd_toEndOf="parent"
                android:id="@+id/textViewOutboundDuration"
                android:layout_margin="8dp"/>

        <!-- inbound  -->
        <ImageView
                android:layout_width="64dp"
                android:layout_height="64dp"
                tools:srcCompat="@tools:sample/avatars"
                app:layout_constraintTop_toBottomOf="@id/imageViewOutBoundCarrier"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginTop="16dp"
                android:layout_marginStart="8dp"
                android:id="@+id/imageViewInboundCarrier"/>
        <TextView
                tools:text="TextView"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:id="@+id/textViewInboundTime"
                app:layout_constraintEnd_toStartOf="@+id/textViewInboundType"
                app:layout_constraintStart_toEndOf="@+id/imageViewInboundCarrier"
                app:layout_constraintTop_toTopOf="@id/imageViewInboundCarrier"
                android:textColor="@android:color/black"
                android:layout_margin="8dp"/>
        <TextView
                tools:text="TextView"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toBottomOf="@id/textViewInboundTime"
                app:layout_constraintStart_toStartOf="@id/textViewInboundTime"
                android:id="@+id/textViewInboundPlacesAndCarrier"
                app:layout_constraintEnd_toStartOf="@+id/textViewInboundDuration" android:layout_marginTop="8dp"/>

        <TextView
                tools:text="TextView"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@id/imageViewInboundCarrier"

                android:textColor="@android:color/black"
                android:id="@+id/textViewInboundType"
                android:layout_margin="8dp"/>
        <TextView
                tools:text="TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toBottomOf="@id/textViewInboundType"
                app:layout_constraintEnd_toEndOf="parent"
                android:id="@+id/textViewInboundDuration"
                android:layout_margin="8dp"/>

        <View
                android:id="@+id/separator1"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/cardview_shadow_start_color"
                app:layout_constraintTop_toBottomOf="@id/imageViewInboundCarrier"
                android:layout_marginTop="4dp"
                android:layout_marginBottom="4dp"
        />
        <TextView
                tools:text="TextView"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@id/separator1"

                android:textColor="@android:color/black"
                android:id="@+id/textViewPrice"
                android:layout_margin="8dp"/>
        <TextView
                tools:text="TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toBottomOf="@id/textViewPrice"
                app:layout_constraintEnd_toEndOf="parent"
                android:id="@+id/textViewAgent"
                android:layout_margin="8dp"
        />
    </androidx.constraintlayout.widget.ConstraintLayout>


</merge>